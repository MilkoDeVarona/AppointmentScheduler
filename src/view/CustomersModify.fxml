<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomersModify">
   <children>
      <Label layoutX="110.0" layoutY="65.0" text="Modify Customer">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <GridPane layoutX="40.0" layoutY="145.0" prefHeight="360.0" prefWidth="320.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="155.0" minWidth="10.0" prefWidth="119.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="217.0" minWidth="10.0" prefWidth="201.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Customer ID" />
            <Label text="Name" GridPane.rowIndex="1" />
            <Label text="Address" GridPane.rowIndex="2" />
            <Label text="Postal Code" GridPane.rowIndex="3" />
            <Label text="Phone" GridPane.rowIndex="4" />
            <Label text="Country" GridPane.rowIndex="5" />
            <Label text="State / Province" GridPane.rowIndex="6" />
            <TextField fx:id="modCustomerID" disable="true" prefHeight="30.0" GridPane.columnIndex="1">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
            <TextField fx:id="addName" prefHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="modAddress" prefHeight="30.0" prefWidth="203.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="modPostalCode" prefHeight="30.0" prefWidth="219.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="modPhone" prefHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <ComboBox fx:id="modCountryCB" prefHeight="30.0" prefWidth="201.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <ComboBox fx:id="modDivisionCB" prefHeight="30.0" prefWidth="201.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
         </children>
      </GridPane>
      <Button layoutX="300.0" layoutY="505.0" mnemonicParsing="false" onAction="#onCancelButton" prefHeight="30.0" prefWidth="60.0" text="Cancel" />
      <Button layoutX="233.0" layoutY="505.0" mnemonicParsing="false" onAction="#onSaveButton" prefHeight="30.0" prefWidth="60.0" text="Save" />
   </children>
</AnchorPane>
