<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <TextField fx:id="userNameTextField" focusTraversable="false" layoutX="99.0" layoutY="130.0" prefHeight="35.0" prefWidth="180.0" promptText="User name" />
      <TextField fx:id="passwordTextField" focusTraversable="false" layoutX="99.0" layoutY="183.0" prefHeight="35.0" prefWidth="180.0" promptText="Password" />
      <Label layoutX="157.0" layoutY="64.0" text="Log In">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="LoginLabel" layoutX="99.0" layoutY="305.0" text="Location" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="99.0" layoutY="244.0" mnemonicParsing="false" onAction="#onSubmitButtonAction" prefHeight="30.0" prefWidth="180.0" text="Click to login" />
   </children>
</AnchorPane>
