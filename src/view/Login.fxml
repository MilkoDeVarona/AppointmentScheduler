<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <TextField fx:id="userNameTextField" focusTraversable="false" layoutX="111.0" layoutY="129.0" prefHeight="35.0" prefWidth="180.0" promptText="User name" />
      <PasswordField fx:id="passwordTextField" layoutX="111.0" layoutY="183.0" prefHeight="35.0" prefWidth="180.0" promptText="Password" />
      <Label layoutX="168.0" layoutY="51.0" text="Log In">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="111.0" layoutY="305.0" text="Location: " textAlignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="111.0" layoutY="242.0" mnemonicParsing="false" onAction="#onSubmitButtonAction" prefHeight="30.0" prefWidth="180.0" text="Click to login" />
      <Label fx:id="loginLabel" layoutX="180.0" layoutY="305.0" text="Label">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="loginErrorLabel" alignment="CENTER" layoutX="9.0" layoutY="94.0" prefHeight="17.0" prefWidth="380.0" style="-fx-text-fill: #FF0000;" />
   </children>
</AnchorPane>
